				local Hum = hit.Parent:FindFirstChildOfClass("Humanoid")
			if not Hum or Hum == Humanoid or IsTeamMate(Creator.Value,Services.Players:GetPlayerFromCharacter(Hum.Parent)) then return end
				Hum:TakeDamage(Hum.Health)
			end)
		end
