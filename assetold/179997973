	if light:IsA("Light") then
		local LightTween = Services.TweenService:Create(light,TweenInfo.new(.5,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0),{Color = Color3.fromRGB(255, 0, 255)})
		LightTween:Play()
	end
end

