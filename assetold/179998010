						<ProtectedString name="Source"><![CDATA[Char = script.Parent
Hum = Char:FindFirstChild("Humanoid")
Head = Char:FindFirstChild("Head")

CreatorTag = script:FindFirstChild("creator")
SpiderMaster = CreatorTag.Value

--SpiderSword = SpiderMaster.Character:FindFirstChild("FangOfAnansi")

if Hum == nil or SpiderMaster == nil or Head == nil then
	script:Destroy()
end

parts = {}
OldClothes = {}
normalMeshes = {}
bodyMeshes = {}
r15bodymeshes = {}
Colors = {}

function TagHumanoid(humanoid, player)
	local Creator_Tag = Instance.new("ObjectValue")
	Creator_Tag.Name = "creator";
	Creator_Tag.Value = player;
	game:GetService("Debris"):AddItem(Creator_Tag, 2)
	Creator_Tag.Parent = humanoid
end

function UntagHumanoid(humanoid)
	for _, v in pairs(humanoid:GetChildren()) do
		if v:IsA("ObjectValue") and v.Name == "creator" then
			v:Destroy()
		end
	end
end

function BlownUp(hit)
	if hit.Parent ~= nil and hit.Parent:FindFirstChild("Humanoid") and not hit.Parent:FindFirstChild("ForceField")then
		UntagHumanoid(hit.Parent.Humanoid)
		TagHumanoid(hit.Parent.Humanoid,SpiderMaster)
	end
	if hit.Anchored == false and not hit:IsDescendantOf(Char) and hit.Name ~= "Handle" and hit.Parent:FindFirstChild("ForceField") == nil then
		hit:BreakJoints()
		hit.Velocity = CFrame.new(SP.Position,hit.Position).lookVector * 500
	end
end

FireTxs = {
	"rbxasset://textures/particles/fire_main.dds",
	"rbxassetid://341300088"
}
AutoDetonators = {
	--"nuclearwave"
	"Lava"
}
stuff = {}
Detonate = false

function CheckForFire(hit)
	stuff = hit:GetDescendants()
	for z = 1,#stuff do
		if stuff[z].ClassName == "Fire" and (stuff[z].Parent:IsA("BasePart") or stuff[z].Parent.ClassName == "Attachment") and stuff[z].Enabled == true then
			Detonate = true
		end
		if stuff[z].ClassName == "ParticleEmitter" and (stuff[z].Parent:IsA("BasePart") or stuff[z].Parent.ClassName == "Attachment") and stuff[z].Enabled == true then
			for y = 1,#FireTxs do
				if stuff[z].Texture == FireTxs[y] then
					Detonate = true
				end
			end
		end
	end
	for x = 1,#AutoDetonators do
		if hit.Name == AutoDetonators[x] then
			Detonate = true
		end
	end
end

ch = Char:GetDescendants()
for a = 1,#ch do
	if ch[a].ClassName == "Shirt" or ch[a].ClassName == "Pants" then
		ch[a].Parent = script
		table.insert(OldClothes,ch[a])
	end
	if ch[a].ClassName == "SpecialMesh" and ch[a].Parent.Name ~= "Head" then
		SV = Instance.new("StringValue")
		SV.Value = ch[a].TextureId
		SV.Name = "oldTX"
		SV.Parent = ch[a]
		table.insert(normalMeshes,SV)
		ch[a].TextureId = ""
	end
	if ch[a].ClassName == "CharacterMesh" then
		SV = Instance.new("StringValue")
		SV.Value = ch[a].OverlayTextureId
		SV.Name = "oldTX"
		SV.Parent = ch[a]
		table.insert(bodyMeshes,SV)
		ch[a].OverlayTextureId = ""
	end
	if ch[a].ClassName == "MeshPart" then
		SV = Instance.new("StringValue")
		SV.Value = ch[a].TextureID
		SV.Name = "oldTX"
		SV.Parent = ch[a]
		BV = Instance.new("BrickColorValue")
		BV.Value = ch[a].BrickColor
		BV.Name = "oldColor"
		BV.Parent = ch[a]
		table.insert(Colors,BV)
		table.insert(r15bodymeshes,SV)
		ch[a].TextureID = ""
		ch[a].Color = Color3.fromRGB(0,255,0)
		ch[a].Material = "Neon"
		table.insert(parts,ch[a])
	end
	if ch[a].ClassName == "Part" then
		BV = Instance.new("BrickColorValue")
		BV.Value = ch[a].BrickColor
		BV.Name = "oldColor"
		BV.Parent = ch[a]
		table.insert(Colors,BV)
		table.insert(parts,ch[a])
		--ch[a].Anchored = true
		ch[a].Color = Color3.fromRGB(0,255,0)
		ch[a].Material = "Neon"
	end
end

for a = 1,#parts do
	parts[a].Touched:connect(CheckForFire)
end

--local AnansiId = 117508233
--local Anansi = Game:GetService('InsertService'):LoadAsset(AnansiId):GetChildren()[1]

Run = game:GetService("RunService")
Debree = game:GetService("Debris")

PoisonTimer = 100

while PoisonTimer > 0 do
	wait(0.1)
	PoisonTimer = PoisonTimer - 1
	UntagHumanoid(Hum)
	TagHumanoid(Hum,SpiderMaster)
	Hum:TakeDamage(0.5)
	if Hum.Health <= 0 or Detonate == true then
		PoisonTimer = 0
	end
	for a = 1,#parts do
		CheckForFire(parts[a])
	end
end

if Detonate == true then
	script.Parent = game.Workspace
	SP = Instance.new("Part")
	SP.Transparency = 1
	SP.CanCollide = false
	SP.Anchored = true
	SP.Position = Head.Position
	SP.Parent = game.Workspace
	Debree:AddItem(SP,4)
	
	ES = script.Explosion:Clone()
	ES.Parent = SP
	ES:Play()
	
	for a = 1,#parts do
		parts[a]:Destroy()
	end
	
	EF = script.ExplosionFire:Clone()
	EF.Size = NumberSequence.new{
		NumberSequenceKeypoint.new(0,1),
		NumberSequenceKeypoint.new(1,50)
	}
	EF.Parent = SP

	ER = script.ExplosionRing:Clone()
	ER.Size = NumberSequence.new{
		NumberSequenceKeypoint.new(0,1),
		NumberSequenceKeypoint.new(1,60)
	}
	ER.Parent = SP
	
	E = Instance.new("Explosion")
	E.Position = SP.Position
	E.BlastRadius = 30
	E.BlastPressure = 100000
	E.DestroyJointRadiusPercent = 0
	E.Parent = game.Workspace
	E.Hit:Connect(BlownUp)

	EF:Emit(30)
	ER:Emit(5)
	return
end

for a = 1,#parts do
	parts[a].Material = "Plastic"
end
for b = 1,#OldClothes do
	OldClothes[b].Parent = Char
end
for c = 1,#normalMeshes do
	if normalMeshes[c].Parent ~= nil then
		normalMeshes[c].Parent.TextureId = normalMeshes[c].Value
	end
	normalMeshes[c]:Destroy()
end
for d = 1,#bodyMeshes do
	if bodyMeshes[d].Parent ~= nil then
		bodyMeshes[d].Parent.OverlayTextureId = bodyMeshes[d].Value
	end
	bodyMeshes[d]:Destroy()
end
for e = 1,#r15bodymeshes do
	if r15bodymeshes[e].Parent ~= nil then
		r15bodymeshes[e].Parent.TextureID = r15bodymeshes[e].Value
		r15bodymeshes[e].Material = "Plastic"
	end
	r15bodymeshes[e]:Destroy()
end
for f = 1,#Colors do
	if Colors[f].Parent ~= nil then
		Colors[f].Parent.BrickColor = Colors[f].Value
	end
	Colors[f]:Destroy()
end

wait(0.7)

script:Destroy()]]></ProtectedString>
