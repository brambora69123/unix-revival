if script:FindFirstChild("creator") == nil then
	Flake:Destroy()
end

Running = game:GetService("RunService")
Debree = game:GetService("Debris")

Plyr = script.creator.Value

Char = Plyr.Character

Tag = script.creator:Clone()
Tag.Parent = Flake
Tag.Changed:connect(function(thing)
	if Tag.Value and Tag.Value.ClassName == "Player" then
		Plyr = Tag.Value
		Char = Plyr.Character
	end
end)

--FlySound = script.Projectile_Fly
ImpactSound = script.Shatter
ImpactSound2 = script.Boom
Flakes = script.SnowBullet
Sparkles = script.SnowSplosion

ImpactSound.Parent = Flake
ImpactSound2.Parent = Flake
Flakes.Parent = Flake
Flakes.Enabled = true

Sparkles.Parent = Flake

Wave = Instance.new("Part")
Wave.Transparency = 1
Wave.Anchored = true
Wave.CanCollide = false

script.WaveTop.Parent = Wave
script.WaveBottom.Parent = Wave

function Frozen(Character) -- Quick check to see if they're not frozen
	for _,v in pairs(game.ServerScriptService:GetChildren()) do
		if v:IsA("Script") and v.Name == "Freeze" and v:FindFirstChild("Target") and v:FindFirstChild("Target").Value == Character then
			return true
		end
	end
	return false
end

Hums = {}
Ignore = false

function WaveHit(part)
	if part.Parent ~= nil and not part:IsDescendantOf(Char) and part.Parent:FindFirstChild("Humanoid") ~= nil and not TeamAlly(part.Parent) and not MinionAlly(part.Parent) and not MinionTeamAlly(part.Parent) then
		Ignore = false
		for z = 1,#Hums do
			if Hums[z] == part.Parent.Humanoid then
				Ignore = true
			end
		end
		if Ignore == false then
			Distance = (Flake.Position - part.Position).magnitude
			UntagHumanoid(part.Parent.Humanoid)
			TagHumanoid(part.Parent.Parent,Plyr)
			part.Parent.Humanoid:TakeDamage((50 - Distance)/1)
			table.insert(Hums,part.Parent.Humanoid)
			if math.random(1,10) == 1 and not Frozen(part.Parent) then
				FS = script.Freeze:Clone()
				FS.Parent = game.ServerScriptService
				FS.Creator.Value = Plyr
				FS.Target.Value = part.Parent
				FS.Disabled = false
			end
		end
	end
end

function BulletHit(hit)
	if hit.Parent ~= nil and not hit:IsDescendantOf(Char) and Flake.Anchored == false and (hit.CanCollide == true or hit.Parent:FindFirstChild("Humanoid")) and not TeamAlly(hit.Parent) and not MinionAlly(hit.Parent) and not MinionTeamAlly(hit.Parent)then
		Flake.Anchored = true
		Flake.Transparency = 1
		Flakes.Enabled = false
		ImpactSound:Play()
		ImpactSound2:Play()
		Sparkles:Emit(200)
		if hit.Parent:FindFirstChild("Humanoid") ~= nil then
			UntagHumanoid(hit.Parent.Humanoid)
			TagHumanoid(hit.Parent.Humanoid,Plyr)
			hit.Parent.Humanoid:TakeDamage(60)
		end
		P = Instance.new("Part")
		P.BrickColor = BrickColor.new("Lime green")
		P.Material = "Neon"
		P.Anchored = true
		P.CanCollide = false
		P.Shape = "Ball"
		P.Size = Vector3.new(1,1,1)
		P.Position = Flake.Position
		P.Parent = Flake
		P.Name = "nuclearwave"
		P.Touched:connect(WaveHit)
		
		Wave.Parent = Flake
		
		for a = 1,40 do
			Running.Heartbeat:wait()
			P.Size = P.Size + Vector3.new(1.2,1.2,1.2)
			P.Transparency = P.Transparency + 0.025
			Wave.Size = Wave.Size + Vector3.new(2.5,0,2.5)
			Wave.Position = P.Position
			Wave.WaveTop.Transparency = P.Transparency
			Wave.WaveBottom.Transparency = P.Transparency
		end
		Wave:Destroy()
		P.Size = Vector3.new(0,0,0)
		wait(0.5)
		P:Destroy()
		wait(2)
		Flake:Destroy()
	end
end

function TagHumanoid(humanoid, player)
	local Creator_Tag = Instance.new("ObjectValue")
	Creator_Tag.Name = "creator"
	Creator_Tag.Value = player
	Debree:AddItem(Creator_Tag, 2)
	Creator_Tag.Parent = humanoid
end

function UntagHumanoid(humanoid)
	for i, v in pairs(humanoid:GetChildren()) do
		if v:IsA("ObjectValue") and v.Name == "creator" then
			v:Destroy()
		end
	end
end

-- team related stuff do not steal

if script:FindFirstChild("TeamAttack") then
	TeamSwitch = script.TeamAttack
end

--if you want to change the settings on team attack, go to the main script

function TeamAlly(person)
	Plyr2 = game.Players:GetPlayerFromCharacter(person)
	if TeamSwitch and TeamSwitch.Value == false and Plyr and Plyr2 and not Plyr.Neutral and Plyr.TeamColor == Plyr2.TeamColor then
		return true
	end
	return false
end

function MinionAlly(tempChar)
	if tempChar:FindFirstChild("Master") ~= nil and tempChar.Master.ClassName == "ObjectValue" and tempChar.Master.Value == Plyr then
		return true
	end
	return false
end

function MinionTeamAlly(tempMinion) -- NOTE: you need the above function for this to work
	TempMaster = tempMinion:FindFirstChild("Master")
	if TempMaster and TempMaster.ClassName == "ObjectValue" and TempMaster.Value and TeamAlly(TempMaster.Value.Character) then
		return true
	end
	return false
end

