				<ProtectedString name="Source"><![CDATA[--Original periastron by Noob3713
--Space nuke!

Tool = script.Parent
Sword = Tool.Handle

Tool.Enabled = true

AnimFolder = Tool.Animations

Trail =Sword.Trail
Trail.Attachment0 = Sword.TopTrail
Trail.Attachment1 = Sword.BottomTrail
Trail.Enabled = false

Uranium = Sword.ParticleAT.Uranium
Uranium.Enabled = true

Running = game:GetService("RunService")
Debree = game:GetService("Debris")

Remote = (Tool:FindFirstChild("Remote") or Instance.new("RemoteEvent"));Remote.Name = "Remote";Remote.Parent = Tool
MouseInput = Tool.MouseInput

function TagHumanoid(humanoid, player)
	local Creator_Tag = Instance.new("ObjectValue")
	Creator_Tag.Name = "creator"
	Creator_Tag.Value = player
	Debree:AddItem(Creator_Tag, 2)
	Creator_Tag.Parent = humanoid
end

function UntagHumanoid(humanoid)
	for i, v in pairs(humanoid:GetChildren()) do
		if v:IsA("ObjectValue") and v.Name == "creator" then
			v:Destroy()
		end
	end
end

-- team related stuff do not steal

if Tool:FindFirstChild("TeamAttack") then
	Tool.TeamAttack:Destroy()
end

TeamSwitch = Instance.new("BoolValue")
TeamSwitch.Value = false
TeamSwitch.Name = "TeamAttack"
TeamSwitch.Parent = Tool

--setting the value to true will allow teammates to damage eachother
--setting the value to false will prevent it
--the value can be changed manually if you want i guess

function TeamAlly(person)
	Plyr2 = game.Players:GetPlayerFromCharacter(person)
	if TeamSwitch.Value == false and Plyr and Plyr2 and not Plyr.Neutral and Plyr.TeamColor == Plyr2.TeamColor then
		return true
	end
	return false
end

function MinionAlly(tempChar)
	if tempChar:FindFirstChild("Master") ~= nil and tempChar.Master.ClassName == "ObjectValue" and tempChar.Master.Value == Plyr then
		return true
	end
	return false
end

function MinionTeamAlly(tempMinion) -- NOTE: you need the above function for this to work
	TempMaster = tempMinion:FindFirstChild("Master")
	if TempMaster and TempMaster.ClassName == "ObjectValue" and TempMaster.Value and TeamAlly(TempMaster.Value.Character) then
		return true
	end
	return false
end

-- end team related stuff

OrigGrip = nil
OrigGrip2 = nil

CurrentDamage = 33

Special1 = true
Special2 = true
Special3 = true
DoingSpecial = false

--character detection stuff do not steal

DetectionAOE = Vector3.new(30,30,30)
TempHums = {}
parts = {}
TempRoot = nil
TempChar = nil
TempHum = nil
Ignore = false
Targets = {}
Distance = 999

function FindCharacters(rangePoint,maxRange)
	TempHums = {}
	Targets = {}
	DetectionAOE = Vector3.new(maxRange,maxRange,maxRange)
	DetectRegion = Region3.new(rangePoint - DetectionAOE,rangePoint + DetectionAOE)
	parts = game.Workspace:FindPartsInRegion3(DetectRegion,Char,math.huge)
	for a = 1,#parts do
		if parts[a].Parent ~= nil and parts[a].Parent:FindFirstChild("Humanoid") and not parts[a].Parent:FindFirstChild("ForceField") and not TeamAlly(parts[a].Parent) and not MinionAlly(parts[a].Parent) and not MinionTeamAlly(parts[a].Parent) then
			TempRoot = parts[a].Parent:FindFirstChild("HumanoidRootPart") or parts[a].Parent:FindFirstChild("Torso")
			TempHum = parts[a].Parent.Humanoid
			TempChar = parts[a].Parent
			Ignore = false
			for h = 1,#TempHums do
				if TempHums[h] == TempHum then
					Ignore = true
				end
			end
			if Ignore == false and TempRoot and TempHum.Health > 0 then
				Distance = (rangePoint - TempRoot.Position).magnitude
				if Distance <= maxRange then
					table.insert(TempHums,TempHum)
					table.insert(Targets,TempChar)
				end
			end
		end
	end
end

function FindCharactersOld(rangePoint, maxRange)
	Targets = {}
	ch = game.Workspace:GetChildren()
	for a = 1,#ch do
		if ch[a]:FindFirstChild("Humanoid") ~= nil and ch[a]:FindFirstChild("Head") ~= nil then
			Distance = (ch[a].Head.Position - rangePoint).magnitude
			if ch[a].Humanoid.Health > 0 and Distance < maxRange and ch[a] ~= Char then
				table.insert(Targets,ch[a])
			end
		end
	end
end

-- end character detection stuff

function BarrelBomb()
	DoingSpecial = true
	LiftAnim = Hum:LoadAnimation(AnimSet.ScytheSummon)
	if LiftAnim then LiftAnim:Play(nil,nil,1.5) end
	
	Barrel = Instance.new("Part")
	Barrel.Color = Color3.fromRGB(255,255,0)
	Barrel.Size = Vector3.new(3,3,3)
	Barrel.CFrame = Root.CFrame * CFrame.Angles(0,0,math.rad(90)) + Root.CFrame.lookVector * 2
	Barrel.TopSurface = "Smooth"
	Barrel.BottomSurface = "Smooth"
	Barrel.Parent = game.Workspace
	Barrel:SetNetworkOwner(nil)
	
	BarrelWeld = Instance.new("Weld")
	BarrelWeld.Part0 = Root
	BarrelWeld.Part1 = Barrel
	BarrelWeld.C0 = CFrame.new(Vector3.new(0,-0.4,-2.5)) * CFrame.Angles(0,0,math.rad(90))
	BarrelWeld.Parent = Barrel
	
	BM = Instance.new("CylinderMesh")
	BM.Parent = Barrel
	
	Face1 = script.NuclearTx:Clone()
	Face1.Parent = Barrel
	
	Face2 = script.NuclearTx:Clone()
	Face2.Face = "Back"
	Face2.Parent = Barrel
	
	for b = 1,2 do
		Ridge = Instance.new("Part")
		Ridge.Color = Color3.fromRGB(255,255,0)
		Ridge.Size = Vector3.new(3.2,0.4,3.2)
		Ridge.CFrame = Barrel.CFrame
		Ridge.TopSurface = "Smooth"
		Ridge.BottomSurface = "Smooth"
		Ridge.Parent = Barrel

		BM = Instance.new("CylinderMesh")
		BM.Parent = Ridge
		if b == 1 then
			BM.Offset = Vector3.new(0,1.5,0)
		else BM.Offset = Vector3.new(0,-1.5,0)
		end
		
		BW = Instance.new("Weld")
		BW.Part0 = Barrel
		BW.Part1 = Ridge
		BW.Parent = Barrel
	end
	wait(0.5)
	LiftAnim:AdjustSpeed(15)
	
	Sword.ThrowSound.TimePosition = 0.3
	Sword.ThrowSound:Play()
	
	BarrelWeld:Destroy()
	
	BV = Instance.new("BodyVelocity")
	BV.Velocity = Vector3.new(0,100,0)
	BV.MaxForce = Vector3.new(1,1,1)*math.huge
	BV.Parent = Barrel
	
	BS = script.BarrelScript:Clone()
	BS.Parent = Barrel
	TeamSwitch:Clone().Parent = BS
	TagHumanoid(BS,Plyr)
	BS.Disabled = false
	wait(1)
	DoingSpecial = false
end

function PeriBomb()
	DoingSpecial = true
	ButtonAnim = Hum:LoadAnimation(AnimSet.ButtonPress)
	if ButtonAnim then ButtonAnim:Play(nil,nil,3) end
	OrigGrip = Tool.Grip
	Tool.Grip = Tool.Grip * CFrame.Angles(0,0,math.rad(90))
	
	FakeBomb = Instance.new("Part")
	FakeBomb.CanCollide = false
	FakeBomb.Size = Vector3.new(0,0,0)
	FakeBomb.Parent = Tool
	FakeBomb.Name = "fusepart"
	
	BombMesh = Instance.new("SpecialMesh")
	BombMesh.MeshId = "http://www.roblox.com/asset/?id=31601976"
	BombMesh.TextureId ="http://www.roblox.com/asset/?id=31601599"
	BombMesh.Scale = Vector3.new(0.7,0.7,0.7)
	BombMesh.Parent = FakeBomb
	
	BombWeld = Instance.new("Weld")
	BombWeld.Part0 = Char:FindFirstChild("Left Arm") or Char:FindFirstChild("LeftHand") or Sword
	BombWeld.Part1 = FakeBomb
	BombWeld.C0 = CFrame.new(Vector3.new(0,-1.3,0)) * CFrame.Angles(0,0,math.rad(90))
	BombWeld.Parent = FakeBomb
	wait(0.3)
	BombWeld.C0 = CFrame.new(Vector3.new(0,0,-2.6)) * CFrame.Angles(math.rad(-90),0,0)
	BombWeld.Part0 = Sword
	wait(0.4)
	--ThrowAnim = Hum:LoadAnimation(AnimSet.Throw)
	--if ThrowAnim then ThrowAnim:Play(nil,nil,1.5) end
	wait(0.3)
	MousePosition = MouseInput:InvokeClient(Plyr)
	Distance = (MousePosition - Sword.Position).magnitude
	if Distance > 200 then
		Distance = 200
	end
	FakePeri = Sword:Clone()
	FakePeri.CanCollide = true
	FakePeri.CFrame = CFrame.new(Sword.Position,MousePosition) * CFrame.Angles(0,0,0)
	--FakePeri.Velocity = FakePeri.CFrame.lookVector * Distance/1.3 + Vector3.new(0,Distance,0)
	FakePeri.Parent = game.Workspace
	BombWeld.Part0 = FakePeri
	FakeBomb.Parent = FakePeri
	FakePeri:SetNetworkOwner(nil)
	
	BS = script.MissileScript:Clone()
	BS.Parent = FakePeri
	TeamSwitch:Clone().Parent = BS
	TagHumanoid(BS,Plyr)
	BS.Disabled = false
	Sword.Transparency = 1
	Uranium.Parent = script
	Tool.Grip = OrigGrip
	
	wait(1)
	for c = 1,40 do
		Running.Heartbeat:wait()
		Sword.Transparency = Sword.Transparency - 0.025
	end
	Sword.Transparency = 0
	Uranium.Parent = Sword.ParticleAT
	wait(0.7)
	DoingSpecial = false
end

StaticTimer = 300
BoltTimer = 15

function ShootSpikes()
	DoingSpecial = true
	SpecialAnim = Hum:LoadAnimation(AnimSet.TonightsForcast)
	if SpecialAnim then SpecialAnim:Play() end
	--Hum.WalkSpeed = 10
	wait(1)
	Lhand = Char:FindFirstChild("Left Arm") or Char:FindFirstChild("LeftHand")
	if Lhand then
		for a = 1,15 do
			FS = Sword.Launch:Clone()
			FS.Parent = Root
			FS:Play()
			Debree:AddItem(FS,2)
			wait(0.2)
			Spike = Instance.new("Part")
			Spike.Anchored = false
			Spike.CanCollide = false
			Spike.Size = Vector3.new(1.2,1.2,1.2)
			Spike.CFrame = Lhand.CFrame * CFrame.Angles(math.pi/-2,0,0)
			Spike.Parent = game.Workspace
			Debree:AddItem(Spike,10)
			Spike:SetNetworkOwner(nil)

			SpikeMesh = Instance.new("SpecialMesh")
			SpikeMesh.MeshId = "http://www.roblox.com/asset/?id=20518365"
			SpikeMesh.TextureId = "http://www.roblox.com/asset/?id=19287687"
			SpikeMesh.Scale = Vector3.new(1.7,1.7,1)
			SpikeMesh.Parent = Spike

			BV = Instance.new("BodyVelocity")
			BV.MaxForce = Vector3.new(1,1,1)*math.huge
			BV.Velocity = CFrame.new(Lhand.Position,Lhand.Position + Root.CFrame.lookVector*1).lookVector * 70 + Vector3.new(math.random(-4,4),math.random(-4,4),math.random(-4,4))
			BV.Parent = Spike
			--BV.P = 1000000
			BV.Name = "vroom"

			BS = script.BulletScript:Clone()
			BS.Parent = Spike
			TeamSwitch:Clone().Parent = BS
			TagHumanoid(BS,Plyr)
			BS.Disabled = false
		end
	end
	wait(1)
	--Hum.WalkSpeed = 16
	DoingSpecial = false
end

function DoSpecial(Client, Key)
	if not Client or Client ~= Plyr or not Key or not Tool.Enabled or Hum.Health <= 0 or DoingSpecial then return end
	if Key == Enum.KeyCode.Q then
		if Special1 == true then
			Special1 = false
			Uranium.Enabled = false
			BarrelBomb()
			wait(12)
			Uranium.Enabled = true
			Special1 = true
		end
	elseif Key == Enum.KeyCode.E then
		if Special2 == true then
			Special2 = false
			PeriBomb()
			wait(10)
			Special2 = true
		end
	elseif Key == Enum.KeyCode.X then
		if Special3 == true then
			Special3 = false
			ShootSpikes()
			wait(10)
			Special3 = true
		end
	end
end

NewAnim = nil
SlashAnim = nil
AnimSpeed = 1
CanLunge = false

LungeBV = nil
LungeStartPos = nil
LungeEndPos = nil
NewC = nil

function Swing()
	if Tool.Enabled == true and DoingSpecial == false then
		--Tool.Enabled = false
		if CanLunge == true then
			--if SlashAnim then SlashAnim:Stop() end
			CanLunge = false
			Tool.Enabled = false
			Sword.LungeSound:Play()
			Trail.Enabled = true
			MousePosition = MouseInput:InvokeClient(Plyr)
			LungeStartPos = Vector3.new(Root.Position.X,0,Root.Position.Z)
			LungeEndPos = Vector3.new(MousePosition.X,0,MousePosition.Z)
			NewC = CFrame.new(LungeStartPos,LungeEndPos)
			LungeBV = Instance.new("BodyVelocity")
			LungeBV.MaxForce = Vector3.new(math.huge,0,math.huge)
			LungeBV.Velocity = NewC.lookVector * 100
			LungeBV.Parent = Root
			Root.CFrame = CFrame.new(LungeStartPos,LungeEndPos) + Vector3.new(0,Root.Position.Y,0)
			Debree:AddItem(LungeBV,0.5)
			wait(0.5)
			Trail.Enabled = false
			wait(1)
			Tool.Enabled = true
		else if SlashAnim then SlashAnim:Stop() end
			NewAnim = Anims[math.random(1,#Anims)]
			SlashAnim = Hum:LoadAnimation(NewAnim)
			if SlashAnim then SlashAnim:Play(nil,nil,AnimSpeed) end
			Sword.SlashSound:Play()
			CanLunge = true
			wait(0.2)
			CanLunge = false
		end
	end
end

Hand = nil
TempHum = nil
TempRoot = nil

FireTxs = {
	"rbxasset://textures/particles/fire_main.dds",
	"rbxassetid://341300088"
}
AutoDetonators = {
	--"nuclearwave"
	"Lava"
}
Detonate = false

function SwordHit(hit)
	if hit == nil or hit.Parent == nil or hit:IsDescendantOf(Char) or Detonate == true or Sword.Transparency > 0 or TeamAlly(hit.Parent) or MinionAlly(hit.Parent) or MinionTeamAlly(hit.Parent) then return end
	
	Hand = (Char:FindFirstChild("Right Arm") or Char:FindFirstChild("RightHand"))
	if Hand ~= nil then
		if Hand:FindFirstChildOfClass("Weld") ~= nil then --problematic but lol
			TempHum = hit.Parent:FindFirstChildOfClass("Humanoid")
			if TempHum ~= nil and TempHum.Health > 0 then
				UntagHumanoid(TempHum)
				TagHumanoid(TempHum,Plyr)
				TempHum:TakeDamage(CurrentDamage)
			end
		end
	end
	stuff = hit:GetDescendants()
	for z = 1,#stuff do
		if stuff[z].ClassName == "Fire" and (stuff[z].Parent:IsA("BasePart") or stuff[z].Parent.ClassName == "Attachment") and stuff[z].Enabled == true then
			Detonate = true
		end
		if stuff[z].ClassName == "ParticleEmitter" and (stuff[z].Parent:IsA("BasePart") or stuff[z].Parent.ClassName == "Attachment") and stuff[z].Enabled == true then
			for y = 1,#FireTxs do
				if stuff[z].Texture == FireTxs[y] then
					Detonate = true
				end
			end
		end
	end
	for x = 1,#AutoDetonators do
		if hit.Name == AutoDetonators[x] then
			Detonate = true
		end
	end
	if Detonate == true then
		BigExplosion()
	end
end

Hums = {}
Ignore = false

function CheckForFire(person)
	stuff = person:GetDescendants()
	for z = 1,#stuff do
		if stuff[z].ClassName == "Fire" and (stuff[z].Parent:IsA("BasePart") or stuff[z].Parent.ClassName == "Attachment") and stuff[z].Enabled == true then
			Detonate = true
		end
		if stuff[z].ClassName == "ParticleEmitter" and (stuff[z].Parent:IsA("BasePart") or stuff[z].Parent.ClassName == "Attachment") and stuff[z].Enabled == true then
			for y = 1,#FireTxs do
				if stuff[z].Texture == FireTxs[y] then
					Detonate = true
				end
			end
		end
	end
end

function Sploded(part)
	if part.Parent ~= nil and not part:IsDescendantOf(Char) and part.Parent:FindFirstChild("Humanoid") ~= nil then
		Ignore = false
		for z = 1,#Hums do
			if Hums[z] == part.Parent.Humanoid then
				Ignore = true
			end
		end
		if Ignore == false then
			UntagHumanoid(part.Parent.Humanoid)
			TagHumanoid(part.Parent.Parent,Plyr)
			table.insert(Hums,part.Parent.Humanoid)
		end
	end
end

function BigExplosion()
	SP = Instance.new("Part")
	SP.Transparency = 1
	SP.Anchored = true
	SP.CanCollide = false
	SP.Size = Vector3.new(0,0,0)
	SP.Position = Sword.Position
	SP.Parent = game.Workspace
	Debree:AddItem(SP,3)
	
	EF = script.MissileScript.ExplosionFire:Clone()
	EF.Size = NumberSequence.new{
		NumberSequenceKeypoint.new(0,1),
		NumberSequenceKeypoint.new(1,50)
	}
	EF.Parent = SP

	ER = script.MissileScript.ExplosionRing:Clone()
	ER.Size = NumberSequence.new{
		NumberSequenceKeypoint.new(0,1),
		NumberSequenceKeypoint.new(1,60)
	}
	ER.Parent = SP

	EF:Emit(30)
	ER:Emit(5)
	
	E = Instance.new("Explosion")
	E.Position = SP.Position
	E.BlastRadius = 30
	E.BlastPressure = 99999999
	E.Parent = game.Workspace
	E.Hit:Connect(Sploded)
	
	Sword.Anchored = true
	Sword.Transparency = 1
	Sword.Explosion:Play()
	wait(3)
	Tool:Destroy()
end

IgnoreDamage = false
NewHealth = 100

DamageReducer = .3

function ReduceDamage()
	NewHealth = Hum.Health
	if not IgnoreDamage and NewHealth ~= Hum.MaxHealth and NewHealth > 0 then
		if NewHealth < CurrentHealth then
			local DamageDealt = (CurrentHealth - NewHealth)
			IgnoreDamage = true
			Hum.Health = Hum.Health + (DamageDealt * DamageReducer)
			IgnoreDamage = false
		end
	end
	CurrentHealth = NewHealth
end

CurrentHeath = 100

Char = nil
Hum = nil
Plyr = nil
Root = nil
AnimSet = nil
function Equip()
	Char = Tool.Parent
	Hum = Char:FindFirstChild("Humanoid")
	Plyr = game.Players:GetPlayerFromCharacter(Char)
	Root = Char:FindFirstChild("HumanoidRootPart") or Char:FindFirstChild("Torso")
	AnimSet = AnimFolder:FindFirstChild(Hum.RigType)
	Anims = {AnimSet.Slash,AnimSet.RightSlash,AnimSet.SlashAnim}
	
	
	--DamageDodge = Hum.Changed:connect(ReduceDamage)
	--CurrentHealth = Hum.Health
	--Ghostify(Char,0.5)
	
	while Tool.Parent == Char and Detonate == false do
		CheckForFire(Char)
		if Detonate == true then
			BigExplosion()
		end
		Running.Heartbeat:wait()
	end
end

function Unequip()
	StaticTimer = 0
	Hum.WalkSpeed = 16
end

